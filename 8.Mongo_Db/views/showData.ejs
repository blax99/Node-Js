<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Students Data</title>
</head>

<body>
    <h1>Students Data</h1>
    <table border="1">
        <tr>
            <td>Name</td>
            <td>Age</td>
            <td>Email</td>
            <td>Operation</td>
        </tr>

        <% students.forEach((student)=>{ %>
            <tr>
                <td>
                    <%= student.name %>
                </td>
                <td>
                    <%= student.age %>
                </td>
                <td>
                    <%= student.email %>
                </td>
                <td>
                    <a href='/ui/delete/<%= student._id%>'>Delete</a>
                </td>
                <td>
                    <a href='/ui/student/<%= student._id%>'>Update</a>
                </td>
            </tr>
            <% })%>
    </table>

    <h2>
        <a href='/form'>Fill Student Data</a>
    </h2>


</body>

</html>